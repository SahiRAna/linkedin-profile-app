{
  "manifest_version": 3,
  "name": "LinkedIn Profile Extractor",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ]
    }
  ]
}